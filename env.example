# ===========================================
# PRZYKŁADOWY PLIK KONFIGURACJI ŚRODOWISKA
# ===========================================
# Skopiuj ten plik do .env i uzupełnij własnymi wartościami

# Wybór środowiska (dev, staging, prod)
NODE_ENV=dev

# URLs dla różnych środowisk
DEV_BASE_URL=https://jsonplaceholder.typicode.com
STAGING_BASE_URL=https://api-staging.example.com
PROD_BASE_URL=https://api.example.com

# API Keys (jeśli potrzebne)
DEV_API_KEY=your-dev-api-key-here
STAGING_API_KEY=your-staging-api-key-here
PROD_API_KEY=your-prod-api-key-here

# Timeouts (w milisekundach)
API_TIMEOUT=30000
RETRY_COUNT=3

# Test configuration
PARALLEL_WORKERS=4
HEADLESS=true

# Debugging
DEBUG_MODE=false
VERBOSE_LOGGING=false

